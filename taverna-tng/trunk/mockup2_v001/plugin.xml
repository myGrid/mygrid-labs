<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         id="application"
         point="org.eclipse.core.runtime.applications">
      <application>
         <run
               class="mockup2_v001.Application">
         </run>
      </application>
   </extension>
   <extension
         point="org.eclipse.ui.perspectives">
      <perspective
            name="RCP Perspective"
            class="mockup2_v001.Perspective"
            id="mockup2_v001.perspective">
      </perspective>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <command
            defaultHandler="TesterCommandHandler"
            id="mockup2_v001.selectionDialog"
            name="Connect">
      </command>
      <command
            defaultHandler="mockup2_v001.commands.SelectionDialog"
            id="mockup2_v001.selectionDialog"
            name="selectionDialog">
      </command>
   </extension>
   <extension
         id="product"
         point="org.eclipse.core.runtime.products">
      <product
            application="mockup2_v001.application"
            name="mockup2_v001">
         <property
               name="windowImages"
               value="icons/alt_window_16.gif,icons/alt_window_32.gif">
         </property>
      </product>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <view
            class="mockup2_v001.TableView"
            id="mockup2_v001.myListView"
            name="myListView"
            restorable="true">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.editors">
      <editor
            class="mockup2_v001.MyGraphicalEditor"
            contributorClass="mockup2_v001.MyGraphicalEditorActionBarContributor"
            default="true"
            id="mockup2_v001.mygraphicaleditor"
            name="MyGraphicalEditor">
      </editor>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            locationURI="menu:mockup2_v001.mainmenu">
         <menu
               id="fileMenu"
               label="File">
         </menu>
      </menuContribution>
   </extension>

</plugin>
